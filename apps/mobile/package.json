{
  "name": "@happy-vue/mobile",
  "version": "0.0.1",
  "private": true,
  "main": "src/app.ts",
  "description": "Happy NativeScript-Vue 3 mobile application for iOS and Android",
  "scripts": {
    "dev:ios": "ns run ios",
    "dev:android": "ns run android",
    "dev:ios:devtools": "ns run ios --env.vueDevtools",
    "dev:android:devtools": "ns run android --env.vueDevtools",
    "build:ios": "ns build ios --release",
    "build:android": "ns build android --release",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "clean": "ns clean && rm -rf dist platforms hooks",
    "prepare": "ns prepare ios && ns prepare android"
  },
  "dependencies": {
    "@happy-vue/protocol": "workspace:*",
    "@happy-vue/shared": "workspace:*",
    "@mleleux/nativescript-revenuecat": "^1.0.3",
    "@nativescript-community/ui-material-activityindicator": "^7.2.78",
    "@nativescript-community/ui-material-bottomnavigationbar": "^7.2.78",
    "@nativescript-community/ui-material-button": "^7.2.78",
    "@nativescript-community/ui-material-core": "^7.2.78",
    "@nativescript-community/ui-material-progress": "^7.2.78",
    "@nativescript-community/ui-material-textfield": "^7.2.78",
    "@nativescript/core": "~9.0.10",
    "@nstudio/nativescript-barcodescanner": "^5.0.1",
    "nativescript-vue": "^3.0.2",
    "pinia": "^3.0.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@nativescript/types": "~9.0.0",
    "@nativescript/webpack": "~5.0.29",
    "typescript": "^5.9.3",
    "vue": "^3.5.26"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
