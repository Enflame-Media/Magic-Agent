// Build settings for Happy iOS app
// Bundle identifier is set in nativescript.config.ts

// Code signing (adjust for your team)
CODE_SIGN_IDENTITY = iPhone Developer
DEVELOPMENT_TEAM =

// Deployment target (iOS 13+)
IPHONEOS_DEPLOYMENT_TARGET = 13.0

// Swift version
SWIFT_VERSION = 5.0

// Enable modules
CLANG_ENABLE_MODULES = YES

// Bitcode
ENABLE_BITCODE = NO
