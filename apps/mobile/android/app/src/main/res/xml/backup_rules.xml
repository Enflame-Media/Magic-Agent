<!--
   Sample backup rules file for Android Backup.
   See https://developer.android.com/guide/topics/data/backup
-->
<full-backup-content>
    <!-- Exclude sensitive data from backup -->
    <exclude domain="sharedpref" path="." />
    <exclude domain="database" path="." />
    <exclude domain="file" path="." />

    <!-- Include only non-sensitive preferences if needed -->
    <!-- <include domain="sharedpref" path="settings.xml" /> -->
</full-backup-content>
